    "from sympy import * \n",
    "from sympy import I\n",
    "\n",
    "L, C, R, w=symbols(\"L, C, R, w\", real=True)\n",
    "ZL, ZC, Z=symbols(\"ZL, ZC, Z\", complex=True)\n",
    "\n",
    "ZL=I*w*L\n",
    "ZC=1/(I*w*C)\n",
    "Z=ZL*ZC/(ZL+ZC) + ZC + R\n"
   ]
  },
